                "properties": {
                    "name": {
                        "title": "Name",
                        "type": "string"
                    },
                    "path": {
                        "title": "Path",
                        "type": "string"
                    },
                    "parent": {
                        "title": "Parent",
                        "type": "string"
                    },
                    "files": {
                        "title": "Files",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/FileEntry"
