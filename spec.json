                    "name": {
                        "title": "Name",
                        "type": "string"
                    },
                    "index": {
                        "title": "Index",
                        "type": "integer"
                    },
                    "hidden": {
                        "title": "Hidden",
                        "type": "boolean"
                    },
                    "editable": {
                        "title": "Editable",
                        "type": "boolean"
                    },
                    "optional": {
                        "title": "Optional",
                        "type": "boolean"
                    },
                    "role": {
                        "title": "Role",
                        "type": "string"
                    },
                    "values": {
                        "title": "Values",
                        "type": "array",
                        "items": {}
