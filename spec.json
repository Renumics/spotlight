                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "title": "Response Get Plugins",
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Plugin"
