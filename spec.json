                "type": "object",
                "properties": {
                    "uid": {
                        "title": "Uid",
                        "type": "string"
                    },
                    "filename": {
                        "title": "Filename",
                        "type": "string"
                    },
                    "columns": {
                        "title": "Columns",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Column"
