                    },
                    "max_rows_hit": {
                        "title": "Max Rows Hit",
                        "type": "boolean"
                    },
                    "max_columns_hit": {
                        "title": "Max Columns Hit",
                        "type": "boolean"
                    },
                    "generation_id": {
                        "title": "Generation Id",
                        "type": "integer"
                    }
