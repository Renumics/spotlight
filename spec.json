            "SetConfigRequest": {
                "title": "SetConfigRequest",
                "type": "object",
                "properties": {
                    "value": {
                        "title": "Value",
                        "anyOf": [
                            {
                                "type": "object"
                            },
                            {
                                "type": "string"
                            },
                            {
                                "type": "integer"
                            },
                            {
                                "type": "number"
                            },
                            {
                                "type": "boolean"
                            }
                        ]
